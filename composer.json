{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/dbal": "^2.7",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/orm": "^2.5.11",
        "prooph/event-sourcing": "^5.5",
        "prooph/event-store-bus-bridge": "^3.2",
        "prooph/event-store-http-middleware": "^0.2.0",
        "prooph/event-store-symfony-bundle": "^0.5.0",
        "prooph/pdo-event-store": "^1.8",
        "prooph/service-bus-symfony-bundle": "^0.7.0",
        "ramsey/uuid": "^3.7",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/var-dumper": "^4.1",
        "symfony/yaml": "^4.1",
        "zendframework/zend-diactoros": "^1.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^4.1",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application",
            "Infrastructure\\": "src/Infrastructure",
            "Domain\\": "src/Domain"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "minimum-stability": "beta"
}
